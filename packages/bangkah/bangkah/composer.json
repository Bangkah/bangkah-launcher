{
  "name": "bangkah/starter",
  "description": "Bangkah Laravel starter kit: interactive scaffolding, Docker, templates",
  "type": "library",
  "license": "MIT",
  "version": "1.0.4",
  "bin": ["bin/bangkah"],
  "autoload": {
    "psr-4": {
      "Bangkah\\Starter\\": "src/"
    },
    "exclude-from-classmap": [
      "/app/",
      "/bootstrap/",
      "/config/",
      "/database/",
      "/public/",
      "/resources/",
      "/routes/",
      "/storage/",
      "/tests/",
      "/vendor/"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bangkah\\Starter\\BangkahServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.4",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/console": "^11.0|^12.0",
    "symfony/process": "^7.0|^6.4"
  },
  "require-dev": {},
  "minimum-stability": "stable",
  "prefer-stable": true
}
